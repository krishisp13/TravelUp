@model List<TravelUp.Models.Product>

<table class="table table-bordered mt-3">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Status</th>
            <th>Category</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@(item.Status ? "Active" : "Inactive")</td>
                    <td>@item.Category</td>
                    <td>@item.Amount.ToString("C")</td>
                </tr>
            }
    </tbody>
</table>
